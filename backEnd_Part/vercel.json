{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      } ,
      
      {
        "src": "/(.*)",
        "dest": "app.js"
      } ,
      {
        "src": "/createProduct",
        "dest": "app.js"
      } ,
      {
        "src": "/createProduct/(.*)",
        "dest": "app.js"
      } ,

      
      {
        "src": "/findAllProducts",
        "dest": "app.js"
      } ,
      {
        "src": "/findAllProducts/(.*)",
        "dest": "app.js"
      } ,

      {
        "src": "/findOneProduct/:productId",
        "dest": "app.js"
      } ,
      {
        "src": "/findOneProduct/:productId/(.*)",
        "dest": "app.js"
      } ,

      {
        "src": "/createReview",
        "dest": "app.js"
      } ,
      {
        "src": "/createReview/(.*)",
        "dest": "app.js"
      } 



    ]
  }



