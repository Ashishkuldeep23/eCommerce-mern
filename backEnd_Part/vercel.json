{
    "version": 2,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/createProduct",
            "dest": "app.js"
        },
        {
            "src": "/createProduct/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/findAllProducts",
            "dest": "app.js"
        },
        {
            "src": "/findAllProducts/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/findOneProduct/:productId",
            "dest": "app.js"
        },
        {
            "src": "/findOneProduct/:productId/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/createReview",
            "dest": "app.js"
        },
        {
            "src": "/createReview/(.*)",
            "dest": "app.js"
        }
    ]
}